{
  "name": "r2upload",
  "version": "1.0.0",
  "description": "API que ir√° enviar arquivos de Backup para o CloudFlare R2 compativel com S3 SDK.",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "concurrently \"npm run watch-compile\" \"npm run wait-and-watch\"",
    "watch-compile": "swc src -w --out-dir dist",
    "wait-and-watch": "wait-on dist/src/server.js && npm run watch-dev",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js dist/src/server.js",
    "build": "npm run clean && swc src -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Express",
    "S3",
    "Cloud",
    "Flare",
    "R2",
    "TypeScript",
    "API"
  ],
  "author": "Lucian Tavares",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.11",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^22.13.10",
    "@types/request": "^2.48.12",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@types/express": "^5.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "keycloak-connect": "^26.1.1",
    "keycloak-verify": "^0.4.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "rate-limiter-flexible": "^6.1.0",
    "ts-node": "^10.9.2"
  }
}
